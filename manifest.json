{
  "manifest_version": 2,
  "name": "Front by FAM",
  "version": "1.0.0",
  "description": "Quick access to Front",

  "browser_action": {
    "default_title": "Front",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["hidden.js"]
    }
  ]
}